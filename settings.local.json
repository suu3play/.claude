{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(git checkout:*)",
      "Bash(gh issue:*)",
      "Bash(gh pr create:*)",
      "Bash(git add:*)",
      "Bash(git push:*)",
      "Bash(git pull:*)",
      "Bash(npm run type-check:*)",
      "Bash(npm run lint)",
      "Bash(npm run build:*)",
      "Bash(npx create-react-app:*)",
      "Bash(npm create:*)",
      "Bash(npm install)",
      "Bash(npm install:*)",
      "Bash(mkdir:*)",
      "Bash(npm run dev:*)",
      "Bash(npm run:*)",
      "Bash(ls:*)",
      "Bash(gh label create:*)",
      "Bash(gh label:*)",
      "Bash(dotnet new:*)",
      "Bash(dotnet sln add:*)",
      "Bash(mv:*)",
      "Bash(dotnet build)",
      "Bash(dotnet add reference:*)",
      "Bash(dotnet add package:*)",
      "Bash(rm:*)",
      "Bash(dotnet run:*)",
      "Bash(timeout:*)",
      "Bash(true)",
      "Bash(for label in \"bug\" \"documentation\" \"duplicate\" \"enhancement\" \"good first issue\" \"help wanted\" \"invalid\" \"question\" \"wontfix\")",
      "Bash(do)",
      "Bash(done)",
      "Bash(cp:*)",
      "Bash(npx tailwindcss init:*)",
      "Bash(./node_modules/.bin/tailwindcss:*)",
      "Bash(npm uninstall:*)",
      "Bash(npm init:*)"
    ],
    "deny": [],
    "defaultMode": "acceptEdits"
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "powershell -Command \"[console]::beep(800, 200); [console]::beep(1000, 150)\""
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "powershell -Command \"[console]::beep(600, 100); [console]::beep(500, 100)\""
          }
        ]
      }
    ]
  }
}